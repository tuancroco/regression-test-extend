        "input": {
          "type": "string"
        },
        "value": {
          "type": "string"
        },
        "append": {
          "type": "boolean"
        },
        "frame": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          ]
        }
      },
      "required": [
        "input",
        "value"
      ]
